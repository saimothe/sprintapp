<%= semantic_form_for @user, :url => process_password_change_admin_user_path(@user) do |f| %>
	<%= f.inputs "Change User Password" do %>
    	<%= f.input :password, :required => true %>
    	<%= f.input :password_confirmation, :label => "Password Confirmation", :required => true %>
  	<% end %>
	<%= f.buttons %>
<% end %>